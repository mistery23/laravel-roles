{
    "name": "mistery23/laravel-roles",
    "description": "Powerful package for handling roles, permissions, and levels in Laravel.",
    "keywords": [
        "laravel-roles",
        "laravel-permissions",
        "laravel-levels",
        "roles",
        "permissions",
        "acl",
        "levels",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Roman Bičan",
            "email": "romanbican@seznam.cz"
        },
        {
            "name": "Jeremy Kenedy",
            "email": "jeremykenedy@gmail.com"
        },
        {
            "name": "Oleksandr Barabolia",
            "email": "alexandrbarabolya@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-pdo": "*",
        "laravel/framework": "~5.8.0|^6.0",
        "illuminate/support": "~5.8.0|^6.0",
        "laravel/helpers": "^1.1",
        "mistery23/eloquent-object-relations": "^1.2",
        "mistery23/eloquent-flusher": "^2.0",
        "ramsey/uuid": "^3.8",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "webmozart/assert": "1.6.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Mistery23\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mistery23\\LaravelRoles\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mistery23\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
