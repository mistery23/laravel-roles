{
    "name": "mistery23/laravel-roles",
    "description": "Powerful package for handling roles, permissions, and levels in Laravel. Supports Laravel 5.3 and higher",
    "keywords": [
        "laravel-roles",
        "laravel-permissions",
        "laravel-levels",
        "roles",
        "permissions",
        "acl",
        "levels",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Roman Bičan",
            "email": "romanbican@seznam.cz"
        },
        {
            "name": "Jeremy Kenedy",
            "email": "jeremykenedy@gmail.com"
        },
        {
            "name": "Oleksandr Barabolia",
            "email": "alexandrbarabolya@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "eklundkristoffer/seedster": "^3.0",
        "laravel/framework": "~5.8.0|^6.0",
        "laravel/helpers": "^1.1",
        "mistery23/eloquent-object-relations": "1.*",
        "mistery23/eloquent-smart-push-relations": "^1.0",
        "ramsey/uuid": "^3.8",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "staudenmeir/laravel-cte": "^1.0",
        "webmozart/assert": "1.6.*"
    },
    "require-dev": {
        "illuminate/support": "^5.8",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.5",
        "laravel/laravel": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Mistery23\\LaravelRoles\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mistery23\\LaravelRoles\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mistery23\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
